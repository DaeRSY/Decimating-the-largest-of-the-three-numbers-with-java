package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // write your code here.
        Scanner scanner = new Scanner(System.in);
        
        int a,b,c;
        
        a = scanner.nextInt(); 
        b = scanner.nextInt();
        c = scanner.nextInt();

        
        if (a>=b && a>=c){
            System.out.println("The largest of the three numbers = "+ a);
        }
        else if (b>=a && b>=c){
            System.out.println("The largest of the three numbers = "+ b);
        }
        else {
            System.out.println("The largest of the three numbers = "+ c);
        }
    }
}
